class Solution {
public:
    bool isPalindrome(int x) {
        long int y=0;
    long int a =x;
if(x<0)
return false;
        while(x!=0){
            y =y*10+x%10;
            x/=10;
      
        }
      if(y==a) 
      return true;
              else
              return false;
              

        
    }
};
